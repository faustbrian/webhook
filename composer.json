{
    "name": "cline/webhook",
    "description": "Standard-compliant Laravel webhook client and server with HMAC and Ed25519 signature support",
    "license": "MIT",
    "keywords": [
        "php",
        "laravel",
        "webhook",
        "standard-webhooks",
        "hmac",
        "ed25519"
    ],
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@cline.sh"
        }
    ],
    "require": {
        "php": "^8.5.0",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/queue": "^12.0",
        "guzzlehttp/guzzle": "^7.9",
        "spatie/laravel-package-tools": "^1.16",
        "paragonie/sodium_compat": "^2.1"
    },
    "require-dev": {
        "orchestra/testbench": "^10.8",
        "symfony/var-dumper": "^7.4.0",
        "pestphp/pest": "^3.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cline\\Webhook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest"
    }
}
